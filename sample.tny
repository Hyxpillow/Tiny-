{ Sample program
  in TINY language -
  computes factorial
}

char A;
char B;

int y;
int fact;
int x0;
char x2;
write x2;
read x0; { input an integer }
if 0 < x0 then { don't compute if x0 <= 0 }
  fact := 1;
  repeat
    fact := fact * x0;
    x0 := x0 - 1
  until x0 = 0;
  write fact  { output factorial of x }
end
